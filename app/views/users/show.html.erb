                      <p><span>Last Name </span>: <%= @user.first_name %></p>
                  </div>
                  <div class="bio-row">
                      <p><span>Country </span>: @user.country </p>
                  </div>
                  <div class="bio-row">
                      <p><span>Birthday</span>: @user.birthday</p>
                  </div>
                  <div class="bio-row">
                      <p><span>Joined </span>: @user.start_date</p>
